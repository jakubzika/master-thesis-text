@article{nguyenOverviewAgentbasedTraffic2021,
  title = {An Overview of Agent-Based Traffic Simulators},
  author = {Nguyen, Johannes and Powers, Simon T. and Urquhart, Neil and Farrenkopf, Thomas and Guckert, Michael},
